            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="PriorityFilterAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/com-netbeansrcp-priorityfilter-PriorityFilterAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="Edit">
            <file name="com-netbeansrcp-priorityfilter-FilterTasksAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Tools/com-netbeansrcp-priorityfilter-FilterTasksAction.instance"/>
