                <attr name="iconBase" stringvalue="com/netbeansrcp/priorityfilter/Filter_16_16.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.awt.Actions.alwaysEnabled"/>
                <attr name="noIconInMenu" boolvalue="false"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="com-netbeansrcp-priorityfilter-PriorityFilterAction.instance">
                <attr name="component" methodvalue="com.netbeansrcp.priorityfilter.PriorityFilterTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="com.netbeansrcp.priorityfilter.Bundle#CTL_PriorityFilterAction"/>
                <attr name="iconBase" stringvalue="com/netbeansrcp/priorityfilter/tc_priorityFilter_16_16.png"/>
