        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="PriorityFilterTopComponent.settings" url="PriorityFilterTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="output">
                <file name="PriorityFilterTopComponent.wstcref" url="PriorityFilterTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
