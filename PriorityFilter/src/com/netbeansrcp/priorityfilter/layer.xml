<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Tools">
            <file name="com-netbeansrcp-priorityfilter-FilterTasksAction.instance">
                <attr name="delegate" newvalue="com.netbeansrcp.priorityfilter.FilterTasksAction"/>
