<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="com-netbeansrcp-overview-OverviewAction.instance">
                <attr name="component" methodvalue="com.netbeansrcp.overview.OverviewTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="com.netbeansrcp.overview.Bundle#CTL_OverviewAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="OverviewAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/com-netbeansrcp-overview-OverviewAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="OverviewTopComponent.settings" url="OverviewTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="explorer">
                <file name="OverviewTopComponent.wstcref" url="OverviewTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
